%{
#include<stdio.h>
#include<string.h>
#include<math.h>
float n=0;
%}
%%

[0,1]   { printf("\nperfect number");}
[0-9]+  {n=atof(yytext);
        float sr = sqrt(n);
        if(sr * sr == n)
	printf("\nperfect square");
	else
	printf("\nnot perfect square");}
	

.*	{ printf("\ninvalid"); }
%%

int yywrap()
{
return 1;}

int main()
{
   
    yylex();
    return 0;
}

